{
  "name": "@adbl/unfinished-start",
  "version": "0.0.6",
  "tasks": {
    "start": "deno run --allow-read --allow-write --allow-run --allow-env --allow-net index.js"
  },
  "compilerOptions": {
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitAny": true,
    "allowUnreachableCode": false,
    "strictFunctionTypes": true,
    "noImplicitReturns": true,
    "allowJs": true,
    "checkJs": true,
    "strict": true
  }
}
